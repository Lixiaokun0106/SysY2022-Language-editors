{
	"name": "lsp-sample-server",
	"description": "Example implementation of a language server in node.",
	"version": "1.0.0",
	"author": "Microsoft Corporation",
	"license": "MIT",
	"engines": {
		"node": "*"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"dependencies": {
		"antlr4": "^4.13.1-patch-1",
		"antlr4ts": "^0.5.0-alpha.4",
		"vscode-languageserver": "^9.0.1",
		"vscode-languageserver-textdocument": "^1.0.11"
	},
	"scripts": {},
	
	
	"contributes": {
        "languages": [
            {
                "id": "SysY2022E",
                "extensions": [".sy"],
                "aliases": ["sysY2022E", "sysY2022E"]
            }
        ],
		"grammars": [
            {
                "language": "SysY2022E",
                "scopeName": "source.sy",
                "path": "./src/syntaxes/sy.tmLanguage.json"
            }
        ],
		"themes": [{
			"label": "SysY2022E",
			"uiTheme": "vs-dark",
			"path": "./src/themes/SysY2022E-color-theme.json"
		}]
    }

}
